{
  "name": "fem-countdown-timer",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/fem-countdown-timer.cjs",
  "module": "./dist/fem-countdown-timer.mjs",
  "exports": {
    ".": {
      "require": "./dist/fem-countdown-timer.cjs",
      "import": "./dist/fem-countdown-timer.mjs"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc  && vite build",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "commit": "commit",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "yarn format:scripts && yarn format:styles"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt-cli": "^17.0.3",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "dts-bundle-generator": "^6.5.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.5.4",
    "vitawind": "^2.2.4",
    "vite": "^2.8.0"
  }
}
